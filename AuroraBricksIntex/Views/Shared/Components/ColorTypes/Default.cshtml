@{
}
@model IQueryable<string>

<div class="d-grid gap-2">
    <a class="btn btn-outline-secondary" asp-controller="Home" asp-action="Index" asp-route-colorType="">Home</a>
    @foreach (string col in Model ?? Enumerable.Empty<string>())
    {
        <a class="btn btn-outline-secondary" asp-controller="Home" asp-action="Index" asp-route-colorType="@col" asp-route-pageNum="1">@col</a>
    }
</div>
